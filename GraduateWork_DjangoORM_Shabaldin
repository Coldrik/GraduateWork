        "from tortoise import Tortoise\n",
        "\n",
        "async def add_issue(request):\n",
        "    if request.method == 'POST':\n",
        "        form = AddIssueForm(request.POST)\n",
        "        if form.is_valid():\n",
        "            issue = await Issue.create(\n",
        "                dwg=form.cleaned_data['dwg'],\n",
        "                concessionRequest=form.cleaned_data['concessionRequest'],\n",
        "                caseOfProblem=form.cleaned_data['caseOfProblem'],\n",
        "                reporter=request.user\n",
        "            )\n",
        "            return redirect('main_page')\n",
        "    else:\n",
        "        form = AddIssueForm()\n",
        "\n",
        "    return render(request, 'add_request.html', {'form': form})\n"
