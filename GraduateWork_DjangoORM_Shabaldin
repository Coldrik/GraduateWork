        "Пример функции для обновления записи с Tortoise ORM:"
      ],
      "metadata": {
        "id": "UlnfF5HyLCje"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "async def update_issue(request, issue_id):\n",
        "    issue = await Issue.get(id=issue_id)\n",
        "    if request.method == 'POST':\n",
        "        form = AddReportForm(request.POST)\n",
        "        if form.is_valid():\n",
        "            issue.concessionReport = form.cleaned_data['concessionReport']\n",
        "            issue.reporter = request.user\n",
        "            await issue.save()  # Сохраняем изменения\n",
        "            return redirect('main_page')\n",
        "    else:\n",
        "        form = AddReportForm(instance=issue)\n",
        "\n",
        "    return render(request, 'add_report.html', {'form': form, 'issue': issue})\n"
